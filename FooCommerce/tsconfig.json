{
  "compilerOptions": {
    "target": "es5",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext",
      "scripthost"
    ],
    "paths": {
      "@/*": [
        "wwwroot/js/react/*"
      ]
    },
    "baseUrl": "./",
    "allowJs": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noFallthroughCasesInSwitch": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": true,
    "jsx": "react-jsx"
  },
  "include": [
    "wwwroot/js/react"
  ],
  "exclude": [
    "node_modules",
    "**/node_modules/*",
    "wwwroot/lib",
    "wwwroot/dist",
    "obj",
    "bin"
  ]
}